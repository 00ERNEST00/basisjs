<b:style src="./native-type-checkbox.css"/>

<input{field|focus}
  type="checkbox"
  class="native-type-checkbox"

  name="{name}"
  value="{value}"
  disabled="{disabled}"
  checked="{checked}"
  readonly="{readonly}"
  indeterminate="{indeterminate}"

  event-focus="focus"
  event-blur="blur"
  event-change="change"
  event-keydown="keydown"
  event-keyup="keyup"
  event-keypress="keypress"
/>
